Sub NoiseDensity_Email()
    Dim objOutlookApp As Object
    Dim objMail As Object

    On Error Resume Next
    Set objOutlookApp = CreateObject("Outlook.Application")
    Set objMail = objOutlookApp.CreateItem(olMailItem)

    With objMail
